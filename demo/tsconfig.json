{
  "compilerOptions": {
    "allowJs": false,
    "lib": [
      "dom",
      "es2015"
    ],
    "jsx": "react",
    "target": "es2015",
    "moduleResolution": "node",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "removeComments": false,
    "preserveConstEnums": false,
    "sourceMap": true,
    "pretty": true
  },
  "include": [
    "./src"
  ]
}
